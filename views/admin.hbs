                                <div class="col-md-9">
                                    <input type="text" name="movTitle" id="movTitle" class="form-control" placeholder="Title">
                                </div>
                            </div>
                            <div class="form-group" class="form-horizontal">
                                <label for="movGenre" class="control-label col-md-3">Genres</label>
                                <div class="col-md-9">
                                    <input type="text" name="movGenre" id="movGenre" class="form-control" placeholder="Genre1, Genre2">
